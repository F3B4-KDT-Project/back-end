{
  "AWSEBDockerrunVersion": "1",
  "ContainerDefinitions": [
    {
      "Name": "app",
      "Image": "groomkdt/kdt:latest",
      "Update": "true",
      "PortMappings": [
        {
          "ContainerPort": 8080,
          "HostPort": 5000
        }
      ]
    },
    {
      "Name": "node-exporter",
      "Image": "prom/node-exporter:master",
      "PortMappings": [
        {
          "ContainerPort": 9100
        }
      ]
    }
  ]
}


